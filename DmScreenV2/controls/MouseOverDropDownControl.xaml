<UserControl x:Class="DmScreenV2.controls.MouseOverDropDownControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DmScreenV2.controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label x:Name="lblTitleText" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Height="55" Width="158" FontSize="16" Background="Gray" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <ComboBox x:Name="cmbItems" HorizontalAlignment="Left" VerticalAlignment="Top" Width="158" Height="55" Visibility="Hidden"/>

    </Grid>
</UserControl>
